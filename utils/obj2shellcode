#!/usr/bin/env bash

objdump -M intel -d $1
echo "==================================="

shellcode=$(objdump --disassemble-zeroes -d $1 | grep -Po '\s\K[a-f0-9]{2}(?=\s)' | sed 's/^/\\x/g' | perl -pe 's/\r?\n//')

echo "SHELLCODE:"
echo $shellcode
echo "-- -- -- -- --"

echo -n "SHELLCODE LENGHT: "
echo -n "$shellcode" | wc -c | awk '{ print $1/2 }'
echo "-- -- -- -- --"

echo "SHELLCODE BYTECODE: "
echo $shellcode | sed 's/\\x/ /g'
echo "-- -- -- -- --"
